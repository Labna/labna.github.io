<script type="text/javascript">
<!-- Ce script masque les √©l√©ments avec les classes "W10","W11","Lin","noW10", "noW11", "noLin" suivant la s√©lection dans le formulaire et devrai rester constant lors de l'impression -->
versions=[".W10",".W11",".Lin"];
function toggleversion(elem){
  // console.log(elem.target.value);
  activClass = elem.target.value;
  if(activClass != "all"){
    vers = versions.filter(e=>{if (e!=activClass) return e})
    vers = vers.concat("no" + activClass)
    // ce qui sera masqu√©
    // dclass = document.getElementsByClassName(".no"+activClass);
    dclass = document.querySelectorAll(vers.join(","));
    for (let i=0; i< dclass.length; i++){
      dclass[i].classList.add("hidden")
    }
  } else {
  	activClass = ".W10,.W11,.Lin"
  }
  // ce qui sera afficher
  // aclass = document.getElementsByClassName(activClass);
  aclass = document.querySelectorAll(activClass);
  for (let i=0; i< aclass.length; i++){
    aclass[i].classList.remove("hidden")
  }
}
document.getElementsByName("version").forEach(elem => {
  elem.addEventListener("change",toggleversion);
})
</script>

